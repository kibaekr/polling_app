<% unless notice.nil? %>
  <div class="bs-alerts-right-fix">
      <p class="alert alert-success">
      <%= notice %>
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      </p>
  </div>
<% end %>
<% unless alert.nil? %>
  <div class="bs-alerts-right-fix">
      <p class="alert alert-danger">
      <%= alert %>
      <button type="button" class="close" data-dismiss="alert">&times;</button>
      </p>    
  </div>
<% end %>
